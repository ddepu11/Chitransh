{
  "parser": "@babel/eslint-parser",
  "ignorePatterns": ["build", "webpack"],

  "extends": [
    "airbnb",
    "airbnb/hooks",
    "prettier",
    "plugin:prettier/recommended"
  ],

  "plugins": ["react", "prettier"],

  "env": { "browser": true, "node": true },

  "rules": {
    "react/react-in-jsx-scope": "off"
  }
}
